import { AfterViewInit, ElementRef } from '@angular/core';
import { DragDrop } from '@angular/cdk/drag-drop';
import { MatTable } from '@angular/material/table';
export declare class CustomHeaderDirective implements AfterViewInit {
    private table;
    private headerRow;
    private dndService;
    private dropListRef;
    private columns;
    constructor(table: MatTable<any>, headerRow: ElementRef, dndService: DragDrop);
    ngAfterViewInit(): void;
    drop(event: any): void;
    private getHeaderCells;
    private createDrags;
    private createDropList;
}
